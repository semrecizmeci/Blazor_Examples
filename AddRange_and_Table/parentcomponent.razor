@page "/parent"
<ChildComponent Items="employees"></ChildComponent>

@code {
    private List<Employee> employees;
    protected override void OnInitialized()
    {
        base.OnInitialized();
        employees = new List<Employee>();
        employees.AddRange(new Employee[3]
        {
            new Employee{Id=1, Name="sado",Title="developer", Department="C#"},
             new Employee{Id=2, Name="mado",Title="developer", Department="C#"},
              new Employee{Id=3, Name="tado",Title="developer", Department="C#"}
        });
    }

    public class Employee
    {
        public int Id { get; set; }
         public string Name { get; set; }
        public string Title { get; set; }
          public string Department { get; set; }
    }
}  

